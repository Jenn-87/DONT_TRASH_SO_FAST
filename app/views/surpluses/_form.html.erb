<%= simple_form_for @surplus, url: surpluses_path do |f| %>
  <%= f.input :photo, as: :file %>
  <%= f.input :name %>
  <%= f.input :category, collection: ["bakery", "butcher", "individual", "ngo", "restaurant"]%>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :quantity %>
  <%= f.submit class: 'btn btn-success' %>
<% end %>
